    "@noble/secp256k1": "^1.7.1",
    "@project-serum/anchor": "^0.26.0",
    "axios": "^1.5.0",
    "bs58": "^5.0.0",
    "js-sha3": "^0.9.2"
  },
  "resolutions": {
    "@solana/buffer-layout": "^4.0.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.3.5"
  }
