{
  "name": "atbash-protocol",
  "version": "1.1.1",
  "main": "dist/app/index.js",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "start": "nodemon app/index.ts",
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
